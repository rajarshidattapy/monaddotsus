# MonadSus Blockchain Configuration
# Copy this file to .env and fill in your values

MONAD_LIVE_MODE=1

# Monad RPC endpoint
MONAD_RPC_URL=https://testnet.monad.xyz/rpc

# Private key for signing game transactions (server-side only)
# For user betting, use the betting.html web interface with MetaMask
MONAD_PRIVATE_KEY=0x...

# Deployed contract addresses (include 0x prefix)
GAME_REGISTRY_ADDRESS="0x01d574966be161f400eeedee00f5afbd6001b6a0"
PREDICTION_MARKET_ADDRESS="0x2ff02c750d9a946420827aa1e4ca007016e987c0"
AGENT_REGISTRY_ADDRESS="0xc52711c6091635b26f1046b1ac40325260b9c9ec"
GAME_RESOLVER_ADDRESS="0x396123b066a81fb7ba4a616fcc6a4b264b4acb3b"

# Persistent Tokenization Contracts (deploy using DeployPersistentTokenization.s.sol)
AGENT_TOKEN_REGISTRY_ADDRESS="0x..."
GAME_PRIZE_POOL_ADDRESS="0x..."
PLATFORM_WALLET_ADDRESS="0x..."

# Tokenization Settings
PRE_GAME_TRADING_DURATION=300  # 5 minutes in seconds
PRIZE_POOL_PERCENTAGE=90       # 90% to winners
PLATFORM_FEE_PERCENTAGE=10     # 10% to platform

# OpenClaw Agent Configuration
AGENT_MODE=simple              # "simple" or "openclaw"
LLM_PROVIDER=openai            # "openai", "anthropic", or "local"
LLM_MODEL=gpt-3.5-turbo        # Model to use
OPENAI_API_KEY=                # Your OpenAI API key
ANTHROPIC_API_KEY=             # Your Anthropic API key
OLLAMA_URL=http://localhost:11434/api/generate  # Local Ollama URL
AGENT_PERSONALITY_DIR=./agent_personalities